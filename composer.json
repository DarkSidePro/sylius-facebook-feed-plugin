{
    "name": "darksidepro/sylius-facebook-feed-plugin",
    "type": "sylius-plugin",
    "version": "1.0.5",
    "description": "Facebook Product Feed generator for Sylius 2.x",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "sylius/sylius": "^2",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DarkSidePro\\SyliusFacebookFeedPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarkSidePro\\SyliusFacebookFeedPlugin\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "symfony": {
            "allow-contrib": false,
            "require": ">=5.4"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
